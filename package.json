{
	"name": "@nortasian/starter",
	"description": "A Simple Node.js Express starter project",
	"version": "1.0.0",
	"license": "Apache-2.0",
	"main": "src/server.ts",
	"scripts": {
		"dev": "nodemon",
		"test": "jest",
		"prepare": "yarn build",
		"build": "yarn clean && npx swc src -d dist",
		"cli": "ts-node --swc -r tsconfig-paths/register",
		"db:seed": "yarn cli ./src/database/seeders",
		"generate:seeder": "yarn cli ./src/commands/generate-seeder",
		"generate:migration": "yarn cli ./src/commands/generate-migration",
		"db:migrate": "NODE_ENV=development; yarn build && npx sequelize db:migrate",
		"db:migrate:undo": "NODE_ENV=development; yarn build && npx sequelize db:migrate:undo",
		"db:migrate:undo:all": "NODE_ENV=development; yarn build && npx sequelize db:migrate:undo:all",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"release": "standard-version",
		"release:pre": "yarn release -- --prerelease",
		"release:patch": "yarn release -- --release-as patch",
		"release:minor": "yarn release -- --release-as minor",
		"release:major": "yarn release -- --release-as major",
		"clean": "rm -rf ./dist"
	},
	"keywords": [
		"express",
		"sequelize",
		"postgres",
		"api",
		"typescript"
	],
	"author": {
		"email": "ahmdswerky@gmail.com",
		"name": "Ahmed Swerky",
		"url": "https://github.com/ahmdswerky"
	},
	"dependencies": {
		"@faker-js/faker": "^7.5.0",
		"@swc/cli": "^0.1.57",
		"@types/compression": "^1.7.2",
		"@types/cors": "^2.8.12",
		"@types/dotenv": "^8.2.0",
		"@types/luxon": "^3.0.2",
		"@types/morgan": "^1.9.3",
		"@types/underscore": "^1.11.4",
		"@types/yargs": "^17.0.13",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.17.1",
		"express-async-handler": "^1.2.0",
		"express-validator": "^6.14.2",
		"helmet": "^6.0.0",
		"jest": "^29.0.3",
		"luxon": "^3.0.4",
		"morgan": "^1.10.0",
		"nodemon": "^2.0.2",
		"pg": "^8.8.0",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.25.2",
		"sequelize-cli": "^6.5.1",
		"sequelize-typescript": "^2.1.5",
		"sha256": "^0.2.0",
		"standard-version": "^9.5.0",
		"supertest": "^6.2.4",
		"ts-jest": "^29.0.3",
		"underscore": "^1.13.6",
		"winston": "^3.8.2",
		"yargs": "^17.6.0"
	},
	"devDependencies": {
		"@eslint/create-config": "^0.3.1",
		"@swc/core": "^1.3.9",
		"@swc/helpers": "^0.4.11",
		"@types/jest": "^29.1.2",
		"@types/node": "^18.8.3",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.39.0",
